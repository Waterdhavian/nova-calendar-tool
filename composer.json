{
    "name": "waterdhavian/nova-calendar-tool",
    "description": "Calendar tool for Laravel Nova.",
    "keywords": [
        "laravel",
        "nova",
        "calendar"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Arno Bor",
            "email": "info@waterdhavian.nl",
            "homepage": "https://www.waterdhavian.nl"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "spatie/laravel-google-calendar": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Waterdhavian\\NovaCalendarTool\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Waterdhavian\\NovaCalendarTool\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
